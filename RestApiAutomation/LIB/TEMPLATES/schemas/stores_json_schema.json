{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "address",
    "address2",
    "city",
    "state",
    "zip",
    "lat",
    "lng",
    "hours"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "address": {
      "$id": "#/properties/address",
      "type": "string",
      "title": "The Address Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "address2": {
      "$id": "#/properties/address2",
      "type": "string",
      "title": "The Address2 Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "city": {
      "$id": "#/properties/city",
      "type": "string",
      "title": "The City Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "state": {
      "$id": "#/properties/state",
      "type": "string",
      "title": "The State Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "zip": {
      "$id": "#/properties/zip",
      "type": "string",
      "title": "The Zip Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "lat": {
      "$id": "#/properties/lat",
      "type": "number",
      "title": "The Lat Schema",
      "default": 0.0,
      "examples": [
        28.365
      ]
    },
    "lng": {
      "$id": "#/properties/lng",
      "type": "number",
      "title": "The Lng Schema",
      "default": 0.0,
      "examples": [
        -96.4567
      ]
    },
    "hours": {
      "$id": "#/properties/hours",
      "type": "string",
      "title": "The Hours Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    }
  }
}