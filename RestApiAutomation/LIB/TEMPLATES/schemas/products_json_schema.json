{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "price",
    "shipping",
    "upc",
    "description",
    "manufacturer",
    "model",
    "url",
    "image"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "price": {
      "$id": "#/properties/price",
      "type": "number",
      "title": "The Price Schema",
      "default": 0.0,
      "examples": [
        5.49
      ]
    },
    "shipping": {
      "$id": "#/properties/shipping",
      "type": "number",
      "title": "The Shipping Schema",
      "default": 0.0,
      "examples": [
        5.49
      ]
    },
    "upc": {
      "$id": "#/properties/upc",
      "type": "string",
      "title": "The Upc Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The Description Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "manufacturer": {
      "$id": "#/properties/manufacturer",
      "type": "string",
      "title": "The Manufacturer Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "model": {
      "$id": "#/properties/model",
      "type": "string",
      "title": "The Model Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "url": {
      "$id": "#/properties/url",
      "type": "string",
      "title": "The Url Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    },
    "image": {
      "$id": "#/properties/image",
      "type": "string",
      "title": "The Image Schema",
      "default": "",
      "examples": [
        "string"
      ],
      "pattern": "^(.*)$"
    }
  }
}