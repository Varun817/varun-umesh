{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "uptime",
    "readonly",
    "documents"
  ],
  "properties": {
    "uptime": {
      "$id": "#/properties/uptime",
      "type": "number",
      "title": "The Uptime Schema",
      "default": 0.0,
      "examples": [
        26675.762
      ]
    },
    "readonly": {
      "$id": "#/properties/readonly",
      "type": "boolean",
      "title": "The Readonly Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "documents": {
      "$id": "#/properties/documents",
      "type": "object",
      "title": "The Documents Schema",
      "required": [
        "products",
        "stores",
        "categories"
      ],
      "properties": {
        "products": {
          "$id": "#/properties/documents/properties/products",
          "type": "integer",
          "title": "The Products Schema",
          "default": 0,
          "examples": [
            51977
          ]
        },
        "stores": {
          "$id": "#/properties/documents/properties/stores",
          "type": "integer",
          "title": "The Stores Schema",
          "default": 0,
          "examples": [
            1586
          ]
        },
        "categories": {
          "$id": "#/properties/documents/properties/categories",
          "type": "integer",
          "title": "The Categories Schema",
          "default": 0,
          "examples": [
            4314
          ]
        }
      }
    }
  }
}